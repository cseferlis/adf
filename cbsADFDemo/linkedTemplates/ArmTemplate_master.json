						"value": "[parameters('srcComplaintsBlob_connectionString')]"
					},
					"src_sqldbAWLT_connectionString": {
						"value": "[parameters('src_sqldbAWLT_connectionString')]"
					},
					"staging_connectionString": {
						"value": "[parameters('staging_connectionString')]"
					},
					"ComplaintsSourceZip_properties_typeProperties_url": {
						"value": "[parameters('ComplaintsSourceZip_properties_typeProperties_url')]"
