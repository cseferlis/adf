		"src_sqldbAWLT_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'src_sqldbAWLT'"
		},
		"staging_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'staging'"
