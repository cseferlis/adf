					"destComplaintsZipBlob_properties_typeProperties_folderPath": {
						"value": "[parameters('destComplaintsZipBlob_properties_typeProperties_folderPath')]"
					},
					"srcComplaintsBlob_properties_typeProperties_fileName": {
						"value": "[parameters('srcComplaintsBlob_properties_typeProperties_fileName')]"
					},
					"srcComplaintsBlob_properties_typeProperties_folderPath": {
						"value": "[parameters('srcComplaintsBlob_properties_typeProperties_folderPath')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "cbsADFDemo_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"srcComplaintsBlobZip_properties_typeProperties_fileName": {
						"value": "[parameters('srcComplaintsBlobZip_properties_typeProperties_fileName')]"
					},
					"srcComplaintsBlobZip_properties_typeProperties_folderPath": {
						"value": "[parameters('srcComplaintsBlobZip_properties_typeProperties_folderPath')]"
					},
					"Daily Update_properties_ComplainsCopyFromWebToBlob_parameters_receiver": {
						"value": "[parameters('Daily Update_properties_ComplainsCopyFromWebToBlob_parameters_receiver')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'cbsADFDemo_ArmTemplate_0')]"
			]
		}
